{
  "model_config": {
    "model_type": "ESMM",
    "feature_groups": {
      "user": ["101", "901", "109_14", "903"],
      "item": ["210", "902", "904"],
      "cross": [{"input_names": ["101", "902"], "hash_bucket_size": 100000}]
    },
    "embedding_dim": 16,
    "wide_output_dim": 16,
    "deep_hidden_units": [128, 64, 32, 8, 1],
    "l2_reg": 0.0001
  },
  "train_config": {
    "epochs": 2,
    "steps_per_epoch":100,
    "loss": ["binary_crossentropy", "binary_crossentropy"],
    "metrics": ["AUC"],
    "optimizer": {
      "name": "adam",
      "learning_rate": {
          "schedule": "exponential_decay",
          "initial_learning_rate": 0.0001,
          "decay_steps": 10000,
          "decay_rate": 0.5
      }
    }
  }
}